  "version": "0.2.0",
  "configurations": [
    {
      "name": "employee",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "args": [
        "--dart-define", 
        "APPLICATION_ENVIRONMENT=dev",
        "--dart-define", 
        "APPLICATION_LOG_LEVEL=info"
      ],
    },
    {
      "name": "employee (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args":[
        "--dart-define", 
        "APPLICATION_ENVIRONMENT=profile",
        "--dart-define", 
        "APPLICATION_LOG_LEVEL=warn"
      ],
    },
    {
      "name": "employee (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": [
        "--dart-define", 
        "APPLICATION_ENVIRONMENT=release",
        "--dart-define", 
        "APPLICATION_LOG_LEVEL=error"
      ],
    }
  ]
}